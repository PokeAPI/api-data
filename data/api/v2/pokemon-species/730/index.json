    "flavor_text_entries": [
        {
            "flavor_text": "\u307f\u305a\u306e\u30d0\u30eb\u30fc\u30f3\u3092\u3000\u3046\u305f\u3067\u3000\u3042\u3084\u3064\u308b\u3002\n\u305d\u306e\u30e1\u30ed\u30c7\u30a3\u306f\u3000\u306a\u304b\u307e\u306b\u3000\u307e\u306a\u3073\n\u3060\u3044\u3060\u3044\u3000\u3080\u308c\u306b\u3000\u3072\u304d\u3064\u304c\u308c\u3066\u3044\u304f\u3002",
