        {
            "flavor_text": "\u3042\u305f\u307e\u306b\u3000\u3059\u308b\u3069\u3044\u3000\u30cf\u30ea\u3092\u3000\u3082\u3064\u3002\n\u3082\u308a\u3084\u3000\u304f\u3055\u3061\u306e\u3000\u3057\u3052\u307f\u306b\u3000\u304b\u304f\u308c\n\u3072\u305f\u3059\u3089\u3000\u30cf\u30c3\u30d1\u3092\u3000\u305f\u3079\u3066\u3044\u308b\u3002",
