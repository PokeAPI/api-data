            }
        },
        {
            "flavor_text": "\ubcc4\uba85\uc740 \ub4dc\ub9b4\ud0b9\uc774\ub2e4.\n\ub545\uc18d\uc744 \ud30c\ub294 \uc18d\ub3c4\ub294\n\uc2dc\uc18d 150km\uc5d0 \ub2ec\ud55c\ub2e4.",
