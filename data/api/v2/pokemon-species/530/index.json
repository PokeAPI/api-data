            "version": {
                "name": "shield",
                "url": "/api/v2/version/34/"
            }
        },
        {
            "flavor_text": "\u5225\u540d\u947d\u982d\u738b\u3002\u5728\u5730\u5e95\u4e0b\n\u6316\u6398\u524d\u9032\u7684\u901f\u5ea6\n\u9ad8\u9054\u6bcf\u5c0f\u6642\uff11\uff15\uff10\u516c\u91cc\u3002",
