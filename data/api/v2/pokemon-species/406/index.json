                "name": "zh-Hans",
                "url": "/api/v2/language/12/"
            },
            "version": {
                "name": "sword",
                "url": "/api/v2/version/33/"
            }
        },
        {
            "flavor_text": "\u304d\u304a\u3093\u306e\u3000\u3078\u3093\u304b\u306b\u3000\u3073\u3093\u304b\u3093\u3002\n\u3064\u307c\u307f\u304c\u3000\u3072\u3089\u304d\u306f\u3058\u3081\u305f\u3089\n\u306f\u308b\u306e\u3000\u304a\u3068\u305a\u308c\u306f\u3000\u3082\u3046\u3059\u3050\u3060\u3002",
