        },
        {
            "flavor_text": "\u30d4\u30f3\u30c1\u306b\u306a\u308b\u3068\u3000\u76ee\u304c\u3000\u6f64\u307f\u3060\u3057\n\u8f1d\u304f\u3002\u3000\u305d\u306e\u5149\u306b\u3000\u7fa4\u308c\u308b\n\u4ef2\u9593\u3068\u3000\u6575\u306b\u3000\u7acb\u3061\u5411\u304b\u3046\u306e\u3060\u3002",
