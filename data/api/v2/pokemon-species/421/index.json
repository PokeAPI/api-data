        {
            "flavor_text": "\u307e\u3093\u304b\u3044\u306e\u3000\u306f\u306a\u3073\u3089\u3000\u304b\u3089\n\u305f\u3060\u3088\u3046\u3000\u304b\u3059\u304b\u306a\u3000\u304b\u304a\u308a\u304c\n\u3080\u3057\u30dd\u30b1\u30e2\u30f3\u3092\u3000\u3042\u3064\u3081\u308b\u3002",
