        {
            "flavor_text": "\u64da\u8aaa\u7260\u6703\u98db\u7051\u706b\u7130\u9c57\u7c89\u9020\u6210\n\u706b\u707d\uff0c\u4e5f\u6709\u8aaa\u6cd5\u8aaa\u7260\u6703\u53bb\n\u6551\u52a9\u53d7\u5bd2\u51b7\u6240\u82e6\u7684\u751f\u7269\u3002",
