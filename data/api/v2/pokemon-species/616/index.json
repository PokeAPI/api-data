        },
        {
            "flavor_text": "\u3067\u3093\u304d\u3000\u30a8\u30cd\u30eb\u30ae\u30fc\u306b\u3000\u306f\u3093\u306e\u3046\u3059\u308b\n\u3075\u3057\u304e\u306a\u3000\u305f\u3044\u3057\u3064\u3002\u3000\u30ab\u30d6\u30eb\u30e2\u3068\n\u3068\u3082\u306b\u3000\u3044\u308b\u3068\u3000\u306a\u305c\u304b\u3000\u3057\u3093\u304b\u3059\u308b\u3002",
