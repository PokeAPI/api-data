    "flavor_text_entries": [
        {
            "flavor_text": "\u4eba\u5de5\u7684\u5b9d\u53ef\u68a6\u3002\n\u56e0\u4e3a\u6709\u66b4\u8d70\u7684\u5371\u9669\uff0c\n\u6240\u4ee5\u88ab\u63a7\u5236\u9762\u7f69\u5c01\u5370\u4f4f\u4e86\u529b\u91cf\u3002",
