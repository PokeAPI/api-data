        {
            "flavor_text": "\u64da\u8aaa\u6703\u5728\u661f\u5149\u9583\u8000\u7684\u591c\u88e1\n\u628a\u7761\u8457\u7684\u5b69\u5b50\u5011\u5e36\u8d70\u3002\n\u4e5f\u6709\u4eba\u7a31\u7260\u70ba\u5236\u88c1\u9b54\u5973\u3002",
