            }
        },
        {
            "flavor_text": "\u628a\u6ef4\u86db\u653e\u9032\u982d\u4e0a\u7684\u6c34\u6ce1\u88e1\uff0c\n\u4e00\u908a\u9935\u7260\u5403\u5269\u7684\u98df\u7269\uff0c\n\u4e00\u908a\u7167\u9867\u7260\u3002",
