        {
            "flavor_text": "\u3042\u305f\u307e\u306e\u3000\u3059\u3044\u307b\u3046\u306e\u3000\u306a\u304b\u306b\n\u30b7\u30ba\u30af\u30e2\u3092\u3000\u3044\u308c\u3066\u3000\u3048\u3055\u306e\u3000\u306e\u3053\u308a\u3092\n\u305f\u3079\u3055\u305b\u306a\u304c\u3089\u3000\u305b\u308f\u3092\u3000\u3059\u308b\u306e\u3060\u3002",
