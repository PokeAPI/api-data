    "flavor_text_entries": [
        {
            "flavor_text": "\u3068\u3046\u3076\u306e\u3000\u3059\u3044\u307b\u3046\u3067\u3000\u30d8\u30c3\u30c9\u30d0\u30c3\u30c8\u3002\n\u3061\u3044\u3055\u306a\u30dd\u30b1\u30e2\u30f3\u3067\u3000\u3042\u308c\u3070\u3000\u305d\u306e\u307e\u307e\n\u3059\u3044\u307b\u3046\u306b\u3000\u3068\u308a\u3053\u307e\u308c\u3000\u304a\u307c\u308c\u3057\u306c\u3002",
