                "url": "/api/v2/language/12/"
            },
            "version": {
                "name": "sword",
                "url": "/api/v2/version/33/"
            }
        },
        {
            "flavor_text": "\u30a8\u30f3\u30cb\u30e5\u30fc\u30c8\u3069\u3046\u3057\u306e\u3000\u3042\u3089\u305d\u3044\u306f\n\u3072\u304d\u3064\u308c\u3066\u3044\u308b\u3000\u30aa\u30b9\u306e\u3000\u30e4\u30c8\u30a6\u30e2\u30ea\u306e\n\u304b\u305a\u3067\u3000\u3057\u3087\u3046\u306f\u3044\u304c\u3000\u304d\u307e\u308b\u3089\u3057\u3044\u3002",
