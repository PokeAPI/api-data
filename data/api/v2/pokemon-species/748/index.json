        },
        {
            "flavor_text": "\u30ac\u30e9\u30eb\u5730\u65b9\u306e\u3000\u51b7\u305f\u3044\u3000\u6c34\u6e29\u306b\n\u8010\u3048\u308b\u305f\u3081\u3000\u811a\u3067\u3000\u30c9\u30fc\u30e0\u3092\u3000\u3064\u304f\u308a\n\u5185\u90e8\u3092\u3000\u4f53\u6e29\u3067\u3000\u6e29\u3081\u308b\u3002",
