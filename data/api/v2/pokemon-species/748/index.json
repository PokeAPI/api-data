            },
            "version": {
                "name": "sword",
                "url": "/api/v2/version/33/"
            }
        },
        {
            "flavor_text": "\u70ba\u4e86\u80fd\u627f\u53d7\u4f4f\u4f3d\u52d2\u723e\u5730\u5340\n\u51b0\u6dbc\u7684\u6c34\u6eab\uff0c\u7260\u7528\u8173\u642d\u8d77\u5713\u9802\uff0c\n\u85c9\u52a9\u9ad4\u6eab\u4fdd\u6301\u5167\u90e8\u7684\u6eab\u6696\u3002",
