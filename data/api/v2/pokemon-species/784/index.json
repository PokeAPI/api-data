        },
        {
            "flavor_text": "\u304b\u305f\u3044\u3000\u30a6\u30ed\u30b3\u306f\u3000\u3053\u3046\u307c\u3046\u3044\u3063\u305f\u3044\u3002\n\u304b\u3064\u3066\u306f\u3000\u3076\u304d\u3084\u3000\u306b\u3061\u3088\u3046\u3072\u3093\u306b\n\u304b\u3053\u3046\u3000\u3055\u308c\u3066\u3000\u3064\u304b\u308f\u308c\u305f\u3002",
