            }
        },
        {
            "flavor_text": "\u63ee\u821e\u5305\u88f9\u8457\u92fc\u9435\u9c57\u7247\u7684\n\u62f3\u982d\u52c7\u6562\u5730\u6230\u9b25\u3002\n\u4e0a\u52fe\u62f3\u662f\u7260\u5f15\u4ee5\u70ba\u50b2\u7684\u7279\u6280\u3002",
