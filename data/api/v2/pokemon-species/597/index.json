        {
            "flavor_text": "\uc774\ub07c \ub080 \ub3d9\uad74\uc744 \uc88b\uc544\ud55c\ub2e4.\n\uc774\ub07c\uc5d0 \ud568\uc720\ub41c \ud6a8\uc18c\uac00 \uac00\uc2dc\ub97c\n\ud06c\uace0 \ud2bc\ud2bc\ud558\uac8c \ub9cc\ub4e4\uc5b4\uc900\ub2e4.",
            "language": {
                "name": "ko",
