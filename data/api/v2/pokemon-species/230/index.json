        },
        {
            "flavor_text": "\ub5a8\uc5b4\uc9c4 \ube44\ub298\uc740 \uc655\uc871\uc5d0\uac8c\n\ud5cc\uc0c1\ub420 \uc815\ub3c4\uc758 \ucd5c\uc0c1\ud488\uc73c\ub85c\n\uc2ec\uc624\ud55c \ube5b\uc744 \ub764\ub2e4.",
            "language": {
