            },
            "version": {
                "name": "ultra-moon",
                "url": "/api/v2/version/30/"
            }
        },
        {
            "flavor_text": "\u4e00\u53ea\u8033\u6735\u4e00\u76f4\u5377\u7740\n\u662f\u4e3a\u4e86\u88ab\u654c\u4eba\u88ad\u51fb\u7684\u65f6\u5019\n\u80fd\u7acb\u5373\u53cd\u51fb\u3002",
