            "version": {
                "name": "sword",
                "url": "/api/v2/version/33/"
            }
        },
        {
            "flavor_text": "\uc591\ucabd \uadc0\ub97c \ub9d0\uace0 \uc788\uc744 \ub54c\ub294\n\ubab8\uc774\ub098 \ub9c8\uc74c\uc774 \uc88b\uc9c0 \uc54a\ub2e4\ub294 \ub73b\uc774\ubbc0\ub85c\n\uad00\ub9ac\uac00 \ud544\uc694\ud558\ub2e4.",
