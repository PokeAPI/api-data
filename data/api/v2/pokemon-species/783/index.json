        },
        {
            "flavor_text": "\u96c4\u53eb\u3073\u3068\u3000\u3068\u3082\u306b\u3000\u7372\u7269\u306b\n\u98db\u3073\u304b\u304b\u308b\u3002\u3000\u30a6\u30ed\u30b3\u306e\u3000\u30d1\u30f3\u30c1\u306f\n\u76f8\u624b\u3092\u3000\u30ba\u30bf\u30ba\u30bf\u306b\u3000\u5f15\u304d\u88c2\u304f\u305e\u3002",
