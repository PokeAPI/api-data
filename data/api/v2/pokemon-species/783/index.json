    "flavor_text_entries": [
        {
            "flavor_text": "\u304a\u305f\u3051\u3073\u3068\u3000\u3068\u3082\u306b\u3000\u3048\u3082\u306e\u306b\n\u3068\u3073\u304b\u304b\u308b\u3002\u3000\u30a6\u30ed\u30b3\u306e\u3000\u30d1\u30f3\u30c1\u306f\n\u3042\u3044\u3066\u3092\u3000\u30ba\u30bf\u30ba\u30bf\u306b\u3000\u3072\u304d\u3055\u304f\u305e\u3002",
