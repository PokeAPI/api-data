            "version": {
                "name": "sword",
                "url": "/api/v2/version/33/"
            }
        },
        {
            "flavor_text": "\u8df3\u306d\u308b\u3088\u3046\u306b\u3000\u6575\u306b\u3000\u8fd1\u3065\u304f\u3068\n\u6bd2\u306e\u30c4\u30e1\u3067\u3000\u3048\u3050\u308b\u3088\u3046\u306b\u3000\u6253\u3064\uff01\n\u304b\u3059\u308a\u50b7\u3067\u3082\u3000\u76f8\u624b\u306f\u3000\uff2b\uff2f\u3060\u3002",
