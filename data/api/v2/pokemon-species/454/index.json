            }
        },
        {
            "flavor_text": "\u5236\u670d\u4e86\u7375\u7269\u4e4b\u5f8c\uff0c\n\u4fbf\u6703\u5471\u5471\u5730\u767c\u51fa\u52dd\u5229\u7684\u6b61\u547c\u3002\n\u8207\u87fe\u870d\u738b\u662f\u76f8\u8fd1\u7684\u7269\u7a2e\u3002",
