            "version": {
                "name": "sun",
                "url": "/api/v2/version/27/"
            }
        },
        {
            "flavor_text": "Da es sich im Schatten verbergen und so f\u00fcr\nmenschliche Augen unsichtbar werden kann,\nwar seine Existenz lange bezweifelt worden.",
            "language": {
                "name": "de",
                "url": "/api/v2/language/6/"
