    "flavor_text_entries": [
        {
            "flavor_text": "\u304b\u3075\u3093\u3092\u3000\u307e\u308b\u3081\u3000\u3060\u3093\u3054\u3092\u3000\u3064\u304f\u308b\u3002\n\u3057\u3087\u304f\u3088\u3046\u3000\u304b\u3089\u3000\u305b\u3093\u3068\u3046\u3088\u3046\u3000\u307e\u3067\n\u305f\u304f\u3055\u3093\u306e\u3000\u3057\u3085\u308b\u3044\u304c\u3000\u3042\u308b\u3088\u3002",
