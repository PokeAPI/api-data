            },
            "version": {
                "name": "ultra-moon",
                "url": "/api/v2/version/30/"
            }
        },
        {
            "flavor_text": "\u62e5\u6709\u5f3a\u5927\u7684\u7cbe\u795e\u529b\u91cf\u3002\n\u4f1a\u7528\u53d1\u7740\uff13\u8272\u5149\u7684\u624b\u6307\n\u64cd\u7eb5\u5bf9\u624b\uff0c\u6539\u5199\u8bb0\u5fc6\u3002",
