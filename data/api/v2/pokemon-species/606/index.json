            },
            "version": {
                "name": "ultra-moon",
                "url": "/api/v2/version/30/"
            }
        },
        {
            "flavor_text": "\u30aa\u30fc\u30d9\u30e0\u304c\u3000\u3042\u3089\u308f\u308c\u305f\u3000\u307c\u304f\u3058\u3087\u3046\n\u304b\u3089\u306f\u3000\u3044\u3064\u306e\u307e\u306b\u304b\u3000\u30d0\u30a4\u30a6\u30fc\u30eb\u30fc\u304c\n\uff11\u3074\u304d\u3000\u3059\u304c\u305f\u3092\u3000\u3051\u3057\u3066\u3057\u307e\u3046\u3002",
