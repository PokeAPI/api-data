        },
        {
            "flavor_text": "\u30aa\u30fc\u30d9\u30e0\u304c\u3000\u73fe\u308c\u305f\u3000\u7267\u5834\n\u304b\u3089\u306f\u3000\u3044\u3064\u306e\u9593\u306b\u304b\u3000\u30d0\u30a4\u30a6\u30fc\u30eb\u30fc\u304c\n\uff11\u5339\u3000\u59ff\u3092\u3000\u6d88\u3057\u3066\u3057\u307e\u3046\u3002",
