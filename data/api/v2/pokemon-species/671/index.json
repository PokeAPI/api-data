    "flavor_text_entries": [
        {
            "flavor_text": "\u6614\u306e\u3000\u57ce\u306e\u3000\u4e3b\u305f\u3061\u306f\u3000\u5ead\u3092\u3000\u98fe\u308b\u305f\u3081\n\u30d5\u30e9\u30fc\u30b8\u30a7\u30b9\u3092\u3000\u62db\u304d\u5165\u308c\n\u82b1\u5712\u3092\u3000\u4f5c\u3089\u305b\u305f\u3002",
