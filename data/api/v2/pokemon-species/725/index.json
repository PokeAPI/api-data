    "flavor_text_entries": [
        {
            "flavor_text": "\u559c\u6b22\u4e0d\u6d41\u9732\u611f\u60c5\uff0c\u72ec\u81ea\u751f\u6d3b\u3002\n\u8981\u53d6\u5f97\u5b83\u7684\u4fe1\u4efb\uff0c\u5f97\u82b1\u70b9\u65f6\u95f4\u3002",
