                "name": "ja",
                "url": "/api/v2/language/11/"
            },
            "version": {
                "name": "sword",
                "url": "/api/v2/version/33/"
            }
        },
        {
            "flavor_text": "\u4e00\u6709\u65f6\u95f4\u5c31\u4f1a\u53bb\u8214\u8eab\u4f53\n\u6574\u7406\u6bdb\u53d1\u3002\u4f1a\u71c3\u70e7\u79ef\u5728\u8179\u4e2d\n\u7684\u843d\u6bdb\u6765\u4f7f\u51fa\u706b\u5c5e\u6027\u62db\u5f0f\u3002",
