        },
        {
            "flavor_text": "\u30cb\u30e3\u30d3\u30fc\u306b\u3000\u3057\u3093\u3089\u3044\u3055\u308c\u308b\u3000\u307e\u3048\u306b\n\u306a\u3067\u3088\u3046\u3068\u3059\u308b\u3068\u3000\u3059\u308b\u3069\u3044\u3000\u30c4\u30e1\u3067\n\u3072\u3069\u304f\u3000\u3072\u3063\u304b\u304b\u308c\u308b\u306e\u3067\u3000\u3061\u3085\u3046\u3044\u3002",
