        {
            "flavor_text": "\u5f9e\u5634\u88e1\u5410\u51fa\u7684\u6ce5\u51dd\u56fa\u5f8c\uff0c\n\u5373\u4f7f\u662f\u98a8\u96e8\u4e5f\u80fd\u64cb\u5f97\u4f4f\uff0c\n\u6240\u4ee5\u904e\u53bb\u5e38\u88ab\u5857\u5728\u5bb6\u5c4b\u7684\u5916\u7246\u4e0a\u3002",
            "language": {
                "name": "zh-Hant",
                "url": "/api/v2/language/4/"
