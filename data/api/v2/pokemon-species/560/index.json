        },
        {
            "flavor_text": "\u3064\u3070\u3092\u3000\u306f\u3044\u3066\u3000\u3042\u3044\u3066\u3092\u3000\u3061\u3087\u3046\u306f\u3064\u3002\n\u304d\u307e\u3063\u305f\u3000\u306a\u308f\u3070\u308a\u3092\u3000\u3082\u3063\u3066\u304a\u308a\n\u3044\u3063\u3057\u3087\u3046\u3000\u305d\u3053\u3092\u3000\u306f\u306a\u308c\u305a\u3000\u3059\u3054\u3059\u3002",
