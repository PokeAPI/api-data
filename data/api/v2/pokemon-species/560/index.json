            }
        },
        {
            "flavor_text": "\u7260\u770b\u4f3c\u7121\u7cbe\u6253\u91c7\u7684\u8e22\u64ca\n\u5176\u5be6\u64c1\u6709\u8db3\u4ee5\u64ca\u788e\u4fee\u5efa\u8001\u5320\n\u624b\u4e2d\u6df7\u51dd\u571f\u7684\u7834\u58de\u529b\u3002",
