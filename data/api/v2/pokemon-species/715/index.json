            },
            "version": {
                "name": "y",
                "url": "/api/v2/version/24/"
            }
        },
        {
            "flavor_text": "Die Ultraschallwellen aus seinen Ohren zermalmen\nFelsen zu kleinen Kieseln. Greift stets im Schutz der\nDunkelheit an.",
            "language": {
                "name": "de",
                "url": "/api/v2/language/6/"
