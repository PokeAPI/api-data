                "url": "/api/v2/language/6/"
            },
            "version": {
                "name": "ultra-moon",
                "url": "/api/v2/version/30/"
            }
        },
        {
            "flavor_text": "De temperamento violento, ataca a todo el que\nse cruce en su camino con unas ondas\nultras\u00f3nicas capaces de partir rocas.",
            "language": {
                "name": "es",
                "url": "/api/v2/language/7/"
