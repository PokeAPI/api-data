        },
        {
            "flavor_text": "\u304b\u3089\u3060\u306e\u3082\u3088\u3046\u3000\u304b\u3089\u3000\u307b\u3093\u3089\u3044\n\u30ac\u30e9\u30eb\u306b\u306f\u3000\u3044\u306a\u3044\u3000\u30dd\u30b1\u30e2\u30f3\u3068\n\u304b\u3093\u304c\u3048\u308b\u3000\u304c\u304f\u3057\u3083\u3082\u3000\u304a\u304a\u3044\u3002",
