        },
        {
            "flavor_text": "\u6b74\u4ee3\u306e\u3000\u738b\u304c\u3000\u9023\u308c\u3066\u3044\u305f\u3002\n\u970a\u529b\u3067\u3000\u4eba\u3084\u3000\u30dd\u30b1\u30e2\u30f3\u306e\n\u5fc3\u3092\u3000\u64cd\u308a\u3000\u5f93\u308f\u305b\u308b\u3002",
