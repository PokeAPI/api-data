        },
        {
            "flavor_text": "\u4f53\u5185\u50a8\u5b58\u7740\u51e0\u767e\u5347\u7684\u6bd2\u6db2\u3002\n\u662f\u88ab\u79f0\u4e3a\u7a76\u6781\u5f02\u517d\u7684\u751f\u7269\u4e2d\u7684\u4e00\u79cd\u3002",
