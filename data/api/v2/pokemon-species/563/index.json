        },
        {
            "flavor_text": "\uc624\ub798\ub41c \ubb18\uc9c0\uc758 \ubcbd\uc744 \ubcf4\uba74\n\uc655\uc758 \ubd80\ub97c \ub4dc\ub7ec\ub0b4\uace0\uc790\n\uadf8\ub824\uc838 \uc788\ub294 \uacbd\uc6b0\uac00 \ub9ce\ub2e4.",
            "language": {
