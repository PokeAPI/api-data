            "version": {
                "name": "sword",
                "url": "/api/v2/version/33/"
            }
        },
        {
            "flavor_text": "Echnatoll haben einen K\u00f6rper aus glitzerndem\nGold. Man geht davon aus, dass sie sich nicht\nmehr an ihr Leben als Menschen erinnern k\u00f6nnen.",
            "language": {
                "name": "de",
                "url": "/api/v2/language/6/"
