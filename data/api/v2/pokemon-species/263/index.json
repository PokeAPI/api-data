        },
        {
            "flavor_text": "\u30ac\u30e9\u30eb\u306e\u3000\u30b8\u30b0\u30b6\u30b0\u30de\u304c\u3000\u307b\u304b\u306e\n\u3061\u3044\u304d\u306b\u3000\u3066\u304d\u304a\u3046\u3057\u305f\u3000\u3059\u304c\u305f\u3002\n\u3055\u304c\u3057\u3082\u306e\u3092\u3000\u307f\u3064\u3051\u308b\u306e\u304c\u3000\u3068\u304f\u3044\u3002",
