        },
        {
            "flavor_text": "\u3059\u306a\u306f\u307e\u306b\u3000\u305f\u304f\u3055\u3093\u3000\u3044\u308b\u3002\u3000\u304b\u3089\u3060\u3092\n\u3064\u3064\u3080\u3000\u30cd\u30d0\u30cd\u30d0\u306e\u3000\u306d\u3093\u3048\u304d\u306f\n\u3072\u3084\u3051\u3042\u3068\u306e\u3000\u30b9\u30ad\u30f3\u30b1\u30a2\u306b\u3000\u3079\u3093\u308a\u3002",
