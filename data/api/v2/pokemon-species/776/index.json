        {
            "flavor_text": "\u306f\u306a\u306e\u3000\u3042\u306a\u304b\u3089\u3000\u307b\u306e\u304a\u3084\u3000\u3069\u304f\u30ac\u30b9\u3092\n\u3075\u304f\u3002\u3000\u30d5\u30f3\u306f\u3000\u3070\u304f\u306f\u3064\u3076\u3064\u3067\n\u3044\u308d\u3093\u306a\u3000\u3064\u304b\u3044\u307f\u3061\u304c\u3000\u3042\u308b\u3002",
