    "flavor_text_entries": [
        {
            "flavor_text": "\u305b\u306a\u304b\u306e\u3000\u3053\u3046\u3089\u306f\u3000\u3070\u304f\u306f\u3064\u3076\u3064\u3002\n\u3046\u3063\u304b\u308a\u3000\u306a\u3050\u308c\u3070\u3000\u3060\u3044\u3070\u304f\u306f\u3064\u3002\n\u304a\u306a\u304b\u306e\u3000\u3042\u306a\u304c\u3000\u3058\u3083\u304f\u3066\u3093\u3002",
