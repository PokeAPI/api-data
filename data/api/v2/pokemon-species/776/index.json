        {
            "flavor_text": "\u304b\u3056\u3093\u306b\u3000\u3059\u307f\u3000\u3044\u304a\u3046\u3084\u3000\u3053\u3046\u3076\u3064\u3092\n\u304f\u3063\u3066\u3044\u308b\u3002\u3000\u304f\u3063\u305f\u30a8\u30b5\u306e\u3000\u305b\u3044\u3076\u3093\u304c\n\u3070\u304f\u306f\u3064\u3059\u308b\u3000\u3053\u3046\u3089\u306e\u3000\u3082\u3068\u306b\u3000\u306a\u308b\u3002",
