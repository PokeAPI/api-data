            },
            "version": {
                "name": "shield",
                "url": "/api/v2/version/34/"
            }
        },
        {
            "flavor_text": "\u50cf\u5f92\u5f1f\u4e00\u6a23\u8ddf\u96a8\u8457\n\u64c5\u9577\u8df3\u821e\u7684\u8e0f\u51b0\u4eba\u5076\uff0c\n\u62fc\u547d\u6a21\u4eff\u7260\u7684\u821e\u6b65\u3002",
