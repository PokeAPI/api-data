                "url": "/api/v2/language/11/"
            },
            "version": {
                "name": "sword",
                "url": "/api/v2/version/33/"
            }
        },
        {
            "flavor_text": "\u4f1a\u6a21\u4eff\u4e00\u5207\u8fdb\u5165\u773c\u5e18\u7684\u4e8b\u7269\u3002\n\u4e00\u5fc3\u4e00\u610f\u6a21\u4eff\u8e0f\u51b0\u4eba\u5076\n\u534e\u4e3d\u7684\u821e\u6b65\uff0c\u4e0d\u505c\u5730\u7ec3\u4e60\u3002",
