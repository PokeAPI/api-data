            }
        },
        {
            "flavor_text": "When it encounters something, its first urge is\nusually to bite it. If it likes what it tastes, it will\ncommit the associated scent to memory.",
            "language": {
                "name": "en",
                "url": "/api/v2/language/9/"
            },
            "version": {
                "name": "sword",
                "url": "/api/v2/version/33/"
            }
        },
        {
            "flavor_text": "\u3068\u308a\u3042\u3048\u305a\u3000\u307e\u305a\u3000\u565b\u307f\u3064\u3044\u3066\u307f\u308b\n\u7fd2\u6027\u3002\u3000\u7f8e\u5473\u3057\u304b\u3063\u305f\u3000\u3082\u306e\u306f\n\u306b\u304a\u3044\u3092\u3000\u3061\u3083\u3093\u3068\u3000\u61b6\u3048\u308b\u3068\u3044\u3046\u3002",
