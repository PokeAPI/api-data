        },
        {
            "flavor_text": "\u591c\u4e2d\u306b\u3000\u30de\u30b7\u30a7\u30fc\u30c9\u306e\u68f2\u3080\u3000\u68ee\u306b\n\u3044\u304f\u306e\u306f\u3000\u5371\u967a\u3002\u3000\u602a\u3057\u3044\u5149\u306b\n\u60d1\u308f\u3055\u308c\u3000\u4e8c\u5ea6\u3068\u3000\u5e30\u308c\u306a\u304f\u306a\u308b\u3002",
