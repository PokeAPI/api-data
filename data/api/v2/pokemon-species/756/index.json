        },
        {
            "flavor_text": "\u6703\u5674\u51fa\u9583\u720d\u7684\u5b62\u5b50\u8b93\u7375\u7269\u7761\u8457\uff0c\n\u518d\u5f9e\u7761\u8457\u7684\u7375\u7269\u8eab\u4e0a\u5438\u53d6\u7cbe\u6c23\u3002",
