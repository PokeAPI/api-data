        },
        {
            "flavor_text": "\u3058\u3076\u3093\u306e\u3000\u304b\u3089\u3060\u3084\u3000\u3059\u306e\u3000\u305d\u3046\u3058\u3067\n\u3088\u3054\u308c\u305f\u3000\u30b7\u30c3\u30dd\u306f\u3000\u304d\u308c\u3044\u306a\n\u308f\u304d\u307f\u305a\u3067\u3000\uff11\u306b\u3061\u3000\u304b\u3051\u3066\u3000\u3042\u3089\u3046\u3002",
            "language": {
                "name": "ja-Hrkt",
                "url": "/api/v2/language/1/"
            },
            "version": {
