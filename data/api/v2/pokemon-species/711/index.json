        {
            "flavor_text": "\ucd08\uc2b9\ub2ec\uc774 \ub72c \ubc24\uc5d0 \uae30\ubd84 \ub098\uc05c \ubaa9\uc18c\ub9ac\ub85c\n\ub178\ub798\ub97c \ubd80\ub974\uba70 \ub9c8\uc744 \uc548\uc744 \ud5e4\ub9e8\ub2e4.\n\uadf8 \ub178\ub798\ub97c \ub4e4\uc73c\uba74 \uc800\uc8fc\ub97c \ubc1b\ub294\ub2e4.",
            "language": {
                "name": "ko",
