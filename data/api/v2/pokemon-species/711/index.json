        },
        {
            "flavor_text": "\u5357\u74dc\u602a\u4eba\u6703\u5728\u6714\u6708\u7684\u591c\u88e1\n\u53bb\u5225\u4eba\u5bb6\u9580\u53e3\u6572\u9580\u3002\u958b\u4e86\u9580\u7684\u4eba\n\u6703\u88ab\u7260\u5e36\u5f80\u53e6\u4e00\u500b\u4e16\u754c\u3002",
            "language": {
                "name": "zh-Hant",
