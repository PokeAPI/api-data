            "version": {
                "name": "sword",
                "url": "/api/v2/version/33/"
            }
        },
        {
            "flavor_text": "This is an ancient Pok\u00e9mon, revived in modern\ntimes. It has a violent disposition, and it\u2019ll tear\napart anything it gets between its hefty jaws.",
            "language": {
                "name": "en",
                "url": "/api/v2/language/9/"
