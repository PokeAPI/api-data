    "flavor_text_entries": [
        {
            "flavor_text": "\u3051\u3044\u304b\u3044\u3057\u3093\u304c\u3000\u3064\u3088\u3044\u3002\u3000\u3072\u308b\u306f\n\u3053\u3046\u3054\u3046\u305b\u3044\u3067\u3000\u3061\u304b\u3089\u3092\u3000\u305f\u3081\u3066\n\u3088\u308b\u306b\u3000\u306a\u3063\u305f\u3089\u3000\u304b\u3064\u3069\u3046\u304b\u3044\u3057\u3002",
