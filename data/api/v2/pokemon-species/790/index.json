    "flavor_text_entries": [
        {
            "flavor_text": "\u3057\u3093\u3060\u3088\u3046\u306b\u3000\u307e\u3063\u305f\u304f\u3000\u3046\u3054\u304b\u306a\u3044\u304c\n\u3075\u308c\u308b\u3068\u3000\u307b\u306e\u304b\u306b\u3000\u3042\u305f\u305f\u304b\u3044\u3002\n\u304a\u304a\u3080\u304b\u3057\u306f\u3000\u307b\u3057\u306e\u30de\u30e6\u3068\u3000\u3088\u3070\u308c\u305f\u3002",
