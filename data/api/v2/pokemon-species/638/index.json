        },
        {
            "flavor_text": "\u30c6\u30e9\u30ad\u30aa\u30f3\u3000\u30d3\u30ea\u30b8\u30aa\u30f3\u3068\u3000\u3068\u3082\u306b\n\u30a4\u30c3\u30b7\u30e5\u5730\u65b9\u306e\u3000\u30dd\u30b1\u30e2\u30f3\u3092\u3000\u5b88\u308a\n\u4eba\u3068\u3000\u6226\u3063\u305f\u3000\u4f1d\u8aac\u304c\u3042\u308b\u3002",
