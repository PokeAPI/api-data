            }
        },
        {
            "flavor_text": "\u751f\u307e\u308c\u306a\u304c\u3089\u3000\u30ea\u30fc\u30c0\u30fc\u306e\u3000\u98a8\u683c\u3092\n\u8eab\u306b\u3064\u3051\u308b\u3002\u3000\u72c2\u66b4\u306a\u3000\u76f8\u624b\u3082\n\u30b3\u30d0\u30eb\u30aa\u30f3\u306e\u3000\u524d\u3067\u306f\u3000\u304a\u3068\u306a\u3057\u3044\u3002",
