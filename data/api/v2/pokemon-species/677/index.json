            "flavor_text": "\u30d7\u30ed\u30ec\u30b9\u30e9\u30fc\u3092\u3000\u3075\u304d\u3068\u3070\u3059\u3000\u307b\u3069\u306e\n\u30b5\u30a4\u30b3\u30d1\u30ef\u30fc\u3092\u3000\u3046\u3061\u306b\u3000\u3072\u3081\u3066\u3044\u308b\u304c\n\u30b3\u30f3\u30c8\u30ed\u30fc\u30eb\u3067\u304d\u305a\u306b\u3000\u3053\u307e\u3063\u3066\u3044\u308b\u3002",
            "language": {
                "name": "ja-Hrkt",
                "url": "/api/v2/language/1/"
            },
            "version": {
