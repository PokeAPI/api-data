            }
        },
        {
            "flavor_text": "\u4ee3\u62c9\u57fa\u7fc1\u70ba\u4fdd\u8b77\u5bf6\u53ef\u5922\n\u800c\u8207\u4eba\u985e\u4f5c\u6230\u7684\u50b3\u8aaa\n\u5728\u5408\u773e\u5730\u5340\u5ee3\u70ba\u6d41\u50b3\u3002",
