        {
            "flavor_text": "\u306a\u307f\u306e\u3000\u304a\u3060\u3084\u304b\u306a\u3000\u3088\u308b\u306b\u306a\u308b\u3068\n\u30ea\u30fc\u30c0\u30fc\u306e\u3000\u30a2\u30b7\u30ec\u30fc\u30cc\u306e\u3000\u3046\u305f\u3054\u3048\u306b\n\u3042\u308f\u305b\u3066\u3000\u306a\u304b\u307e\u305f\u3061\u3068\u3000\u304a\u3069\u308b\u3002",
