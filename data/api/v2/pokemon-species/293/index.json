        },
        {
            "flavor_text": "\u306a\u304d\u306f\u3058\u3081\u308b\u3068\u3000\u3058\u3076\u3093\u306e\u3000\u3053\u3048\u306b\n\u304a\u3069\u308d\u3044\u3066\u3000\u3055\u3089\u306b\u3000\u306f\u3052\u3057\u304f\u3000\u306a\u304f\u3002\n\u306a\u304d\u3064\u304b\u308c\u308b\u3068\u3000\u306d\u3080\u3063\u3066\u3057\u307e\u3046\u3002",
