        },
        {
            "flavor_text": "\u3084\u307e\u304a\u304f\u306b\u3000\u3053\u3082\u308a\u3000\u3080\u308c\u306e\u3000\u306a\u304b\u307e\u3068\n\u3057\u3085\u304e\u3087\u3046\u306b\u3000\u306f\u3052\u3080\u3002\u3000\u3080\u308c\u306b\u3088\u3063\u3066\n\u30ad\u30c3\u30af\u3084\u3000\u30c1\u30e7\u30c3\u30d7\u306e\u304b\u305f\u304c\u3000\u3061\u304c\u3046\u3002",
