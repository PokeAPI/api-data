            },
            "version": {
                "name": "sword",
                "url": "/api/v2/version/33/"
            }
        },
        {
            "flavor_text": "\uc798 \ub2e8\ub828\ub41c \ube44\uc870\ud478\ub294\n1\ubd84\uc5d0 100\ud68c \uc774\uc0c1\n\ucd19 \uacf5\uaca9\uc744 \ud560 \uc218 \uc788\ub2e4.",
            "language": {
                "name": "ko",
