            "version": {
                "name": "shield",
                "url": "/api/v2/version/34/"
            }
        },
        {
            "flavor_text": "\u5f9e\u65e9\u5230\u665a\u90fd\u5728\u63a1\u96c6\u82b1\u871c\u3002\n\u9019\u4e00\u5207\u90fd\u662f\u70ba\u4e86\n\u81ea\u5df1\u7684\u65cf\u7fa4\u548c\u8702\u5973\u738b\u3002",
