            }
        },
        {
            "flavor_text": "\u308c\u3044\u305b\u3044\u3000\u304b\u3064\u3000\u3057\u3093\u3061\u3087\u3046\u3002\u3000\n\u305f\u3066\u304c\u307f\u306e\u3000\u3044\u308f\u306e\u3000\u304d\u308c\u3042\u3058\u306f\n\u304b\u307f\u305d\u308a\u306e\u3088\u3046\u306b\u3000\u3059\u308b\u3069\u3044\u3002",
