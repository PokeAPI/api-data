        },
        {
            "flavor_text": "\u30d4\u30f3\u30af\u8272\u306e\u3000\u6a21\u69d8\u306f\u3000\u591c\u306b\u306a\u308b\u3068\n\u5149\u308a\u3060\u3059\u3002\u3000\u30c0\u30a4\u30d0\u30fc\u306b\u3000\u4eba\u6c17\u306a\u306e\u3067\n\u30ea\u30be\u30fc\u30c8\u5730\u3067\u306f\u3000\u990c\u4ed8\u3051\u3055\u308c\u308b\u3053\u3068\u3082\u3002",
