        {
            "flavor_text": "\u3072\u304b\u308b\u3000\u304a\u3073\u308c\u3067\u3000\u3048\u3082\u306e\u3092\u3000\u3055\u305d\u3046\u3002\n\u3072\u308b\u306f\u3000\u304b\u3044\u3081\u3093\u3000\u3061\u304b\u304f\u306b\u3000\u3044\u3066\n\u3088\u308b\u306b\u3000\u306a\u308b\u3068\u3000\u3075\u304b\u307f\u306b\u3000\u3044\u3069\u3046\u3002",
