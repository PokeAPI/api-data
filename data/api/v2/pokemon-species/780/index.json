        },
        {
            "flavor_text": "\u5fc3\u512a\u3057\u3044\u3000\u6027\u8cea\u3002\u3000\u3060\u304c\n\u4e00\u5ea6\u3000\u6012\u308a\u306b\u3000\u304b\u3089\u308c\u308b\u3068\u3000\u6fc0\u3057\u3044\n\u606f\u5439\u3067\u3000\u3042\u305f\u308a\u3092\u3000\u7834\u58ca\u3000\u3057\u5c3d\u304f\u3059\u3002",
