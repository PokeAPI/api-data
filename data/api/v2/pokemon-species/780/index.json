            }
        },
        {
            "flavor_text": "\u5fc3\u5730\u975e\u5e38\u5584\u826f\u3002\u4e00\u65e6\u88ab\u6fc0\u6012\uff0c\n\u5c31\u6703\u7528\u975e\u5e38\u731b\u70c8\u7684\u6c23\u606f\n\u628a\u5468\u570d\u7834\u58de\u6b86\u76e1\u3002",
