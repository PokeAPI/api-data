    "flavor_text_entries": [
        {
            "flavor_text": "\u3044\u304d\u3060\u304a\u308c\u305f\u3082\u306e\u306e\u3000\u304a\u3093\u306d\u3093\u304c\n\u3053\u3069\u3082\u304c\u3000\u3064\u304f\u3063\u305f\u3000\u3059\u306a\u3084\u307e\u306b\n\u3068\u308a\u3064\u304d\u3000\u305f\u3093\u3058\u3087\u3046\u3000\u3057\u305f\u306e\u3060\u3002",
