            }
        },
        {
            "flavor_text": "\u5c07\u706b\u7130\u7576\u6210\u820c\u982d\u4f86\u4f7f\u7528\u3002\n\u6703\u4e00\u9ede\u4e00\u9ede\u5730\u7194\u5316\u9435\u87fb\n\u5805\u786c\u7684\u5916\u9aa8\u9abc\uff0c\u7136\u5f8c\u628a\u7260\u5403\u6389\u3002",
            "language": {
                "name": "zh-Hant",
