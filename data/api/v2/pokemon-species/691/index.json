        },
        {
            "flavor_text": "\u6d77\u85fb\u306b\u3000\u307e\u304e\u308c\u3066\u3000\u7372\u7269\u3092\u3000\u5f85\u3061\n\u91d1\u5c5e\u3092\u3000\u6eb6\u304b\u3059\u307b\u3069\u306e\u3000\u6bd2\u6db2\u3092\n\u5439\u304d\u304b\u3051\u3066\u3000\u4ed5\u7559\u3081\u308b\u306e\u3060\u3002",
