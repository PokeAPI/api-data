            "version": {
                "name": "sword",
                "url": "/api/v2/version/33/"
            }
        },
        {
            "flavor_text": "Utiliza su veneno capaz de corroer el metal\npara enviar a pique cualquier nav\u00edo que ose\nadentrarse en las aguas que habita.",
            "language": {
                "name": "es",
                "url": "/api/v2/language/7/"
