            }
        },
        {
            "flavor_text": "\u3057\u3087\u304f\u3057\u3085\u3000\u304b\u3089\u3000\u3044\u306a\u305a\u307e\u306e\n\u3088\u3046\u306a\u3000\u30d3\u30fc\u30e0\u3092\u3000\u3075\u308a\u305d\u305d\u304c\u305b\u308b\u3002\n\u3042\u3089\u3076\u308b\u3000\u3081\u304c\u307f\u3000\u3068\u3082\u3000\u3088\u3070\u308c\u308b\u3002",
