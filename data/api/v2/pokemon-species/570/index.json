                "name": "it",
                "url": "/api/v2/language/8/"
            },
            "version": {
                "name": "ultra-sun",
                "url": "/api/v2/version/29/"
            }
        },
        {
            "flavor_text": "This Pok\u00e9mon has a cowardly disposition, so\nwhen it\u2019s not around friends, it basically\nalways stays transformed as something else.",
            "language": {
                "name": "en",
                "url": "/api/v2/language/9/"
            },
            "version": {
                "name": "ultra-sun",
                "url": "/api/v2/version/29/"
            }
        },
        {
            "flavor_text": "\u81c6\u75c5\u306a\u3000\u6027\u8cea\u306e\u3000\u30dd\u30b1\u30e2\u30f3\u3067\n\u4ef2\u9593\u3068\u3000\u3044\u308b\u3068\u304d\u3000\u4ee5\u5916\u306f\u3000\u307b\u307c\n\u4ed6\u306e\u3000\u8ab0\u304b\u306b\u3000\u5316\u3051\u3066\u3044\u308b\u3002",
