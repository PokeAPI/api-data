    "flavor_text_entries": [
        {
            "flavor_text": "\u3042\u3056\u3084\u304b\u306a\u3000\u305f\u3044\u3057\u3087\u304f\u3092\u3000\u305f\u3082\u3064\u306b\u306f\n\u3072\u3058\u3087\u3046\u306b\u3000\u3066\u307e\u304c\u3000\u304b\u304b\u308b\u304c\u3000\u305d\u308c\u3092\n\u3057\u3085\u307f\u306b\u3059\u308b\u3000\u3053\u3046\u305a\u304b\u3082\u3000\u3044\u308b\u306e\u3060\u3002",
