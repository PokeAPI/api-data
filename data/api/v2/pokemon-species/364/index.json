        },
        {
            "flavor_text": "\u30bf\u30de\u30b6\u30e9\u30b7\u3084\u3000\u30e2\u30f3\u30b9\u30bf\u30fc\u30dc\u30fc\u30eb\u306a\u3069\n\u307e\u308b\u3044\u3000\u3082\u306e\u306a\u3089\u3000\u306a\u3093\u3067\u3082\u3000\u306f\u306a\u306e\n\u3046\u3048\u3067\u3000\u307e\u308f\u3057\u3066\u3057\u307e\u3046\u3000\u3057\u3085\u3046\u305b\u3044\u3002",
