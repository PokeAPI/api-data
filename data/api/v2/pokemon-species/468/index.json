        },
        {
            "flavor_text": "\u4e89\u3044\u4e8b\u3084\u3000\u3082\u3081\u4e8b\u304c\u3000\u8d77\u3053\u308b\n\u5834\u6240\u306b\u306f\u3000\u59ff\u3092\u3000\u898b\u305b\u306a\u3044\u3002\n\u8fd1\u3054\u308d\u306f\u3000\u307b\u3068\u3093\u3069\u3000\u898b\u304b\u3051\u306a\u3044\u3002",
