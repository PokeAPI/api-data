            }
        },
        {
            "flavor_text": "\u3081\u3050\u307f\u3092\u3000\u3042\u305f\u3048\u308b\u3000\u305d\u3093\u3056\u3044\u3068\n\u3044\u308f\u308c\u3066\u304a\u308a\u3000\u304a\u304a\u3080\u304b\u3057\u304b\u3089\n\u3048\u3093\u304e\u3082\u306e\u306b\u3000\u3048\u304c\u304b\u308c\u3066\u304d\u305f\u3002",
