    "flavor_text_entries": [
        {
            "flavor_text": "\u30af\u30cd\u30af\u30cd\u3046\u3054\u3044\u3066\u3000\u3072\u3068\u306b\u3000\u3061\u304b\u3065\u304f\u3068\n\u3068\u3064\u305c\u3093\u3000\u3042\u305f\u307e\u3092\u3000\u3070\u304f\u306f\u3064\u3055\u305b\u305f\u3002\n\u30a6\u30eb\u30c8\u30e9\u30d3\u30fc\u30b9\u30c8\u306e\u3000\u3044\u3063\u3057\u3085\u3089\u3057\u3044\u3002",
