            }
        },
        {
            "flavor_text": "\u6703\u626d\u52d5\u8457\u8eab\u9ad4\u9760\u8fd1\u4eba\uff0c\n\u7136\u5f8c\u7a81\u7136\u8b93\u982d\u7206\u70b8\u3002\n\u4f3c\u4e4e\u662f\u7a76\u6975\u7570\u7378\u7684\u4e00\u7a2e\u3002",
            "language": {
                "name": "zh-Hant",
                "url": "/api/v2/language/4/"
            },
