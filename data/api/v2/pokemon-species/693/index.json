            },
            "version": {
                "name": "ultra-moon",
                "url": "/api/v2/version/30/"
            }
        },
        {
            "flavor_text": "\u53f3\u81c2\u975e\u5e38\u7d27\u5b9e\u9971\u6ee1\u3002\n\u6389\u843d\u4e0b\u6765\u7684\u94b3\u5b50\u4f5c\u4e3a\u98df\u6750\n\u88ab\u9500\u5f80\u6d77\u5916\u3002",
