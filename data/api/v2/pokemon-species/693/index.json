            },
            "version": {
                "name": "sword",
                "url": "/api/v2/version/33/"
            }
        },
        {
            "flavor_text": "\u901a\u8fc7\u5de8\u5927\u94b3\u5b50\u7684\u89e6\u89d2\n\u611f\u77e5\u730e\u7269\u7684\u4f4d\u7f6e\uff0c\n\u7136\u540e\u5bf9\u5176\u53d1\u5c04\u6c34\u70ae\u5f39\u3002",
