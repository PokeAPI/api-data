            "version": {
                "name": "shield",
                "url": "/api/v2/version/34/"
            }
        },
        {
            "flavor_text": "Die an Speiseeis erinnernden Beeren, die an\nseinem Bauch wachsen, sind bei den Flampion\nGalars ein sehr beliebter Snack.",
            "language": {
                "name": "de",
                "url": "/api/v2/language/6/"
