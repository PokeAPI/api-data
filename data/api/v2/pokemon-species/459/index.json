        },
        {
            "flavor_text": "\u304a\u306a\u304b\u306b\u3000\u307f\u306e\u308b\u3000\u30a2\u30a4\u30b9\u3000\u307f\u305f\u3044\u306a\n\u304d\u306e\u307f\u306f\u3000\u30ac\u30e9\u30eb\u306b\u3000\u304f\u3089\u3059\n\u30c0\u30eb\u30de\u30c3\u30ab\u305f\u3061\u306e\u3000\u3060\u3044\u3053\u3046\u3076\u3064\u3002",
