            },
            "version": {
                "name": "alpha-sapphire",
                "url": "/api/v2/version/26/"
            }
        },
        {
            "flavor_text": "Es wurde von einer uralten Kultur aus Lehm erschaffen\nund kann mehrere Jahrtausende in Bewegung bleiben.",
            "language": {
                "name": "de",
                "url": "/api/v2/language/6/"
            },
            "version": {
                "name": "alpha-sapphire",
                "url": "/api/v2/version/26/"
