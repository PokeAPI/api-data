    "flavor_text_entries": [
        {
            "flavor_text": "\u628a\u85e4\u8513\u4e0a\u9644\u7740\u7684\u82b1\u6f02\u6d6e\u5728\u6d74\u76c6\u91cc\uff0c\n\u6ce1\u6fa1\u65f6\u4f1a\u6709\u8212\u7f13\u7684\u6548\u679c\uff0c\n\u5728\u5e7f\u5927\u5973\u6027\u4e2d\u6781\u5177\u4eba\u6c14\u3002",
            "language": {
                "name": "zh-Hans",
