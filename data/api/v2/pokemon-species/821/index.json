            "version": {
                "name": "sword",
                "url": "/api/v2/version/33/"
            }
        },
        {
            "flavor_text": "\u3061\u3044\u3055\u3044\u3000\u304b\u3089\u3060\u3067\u3000\u304d\u3073\u3093\u306b\u3000\u3068\u3093\u3067\n\u3059\u304d\u3092\u3000\u3064\u304f\u3000\u305b\u3093\u307d\u3046\u3067\u3000\u304b\u3089\u3060\u306e\n\u304a\u304a\u304d\u306a\u3000\u3042\u3044\u3066\u3092\u3000\u3066\u3060\u307e\u306b\u3068\u308b\u3002",
