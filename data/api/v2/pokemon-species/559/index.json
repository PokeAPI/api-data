                "name": "ko",
                "url": "/api/v2/language/3/"
            },
            "version": {
                "name": "shield",
                "url": "/api/v2/version/34/"
            }
        },
        {
            "flavor_text": "\u7528\u7d50\u5be6\u7684\u76ae\u4fdd\u8b77\u8eab\u9ad4\u3002\n\u64da\u8aaa\u5728\u76ae\u4f38\u5c55\u5230\u6975\u9650\u6642\uff0c\n\u7260\u5c31\u6703\u958b\u59cb\u9032\u5316\u3002",
