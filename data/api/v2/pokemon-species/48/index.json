            "version": {
                "name": "lets-go-pikachu",
                "url": "/api/v2/version/31/"
            }
        },
        {
            "flavor_text": "Seine gro\u00dfen Augen fungieren als Radar.\nIm Licht kann man erkennen, dass sie aus\nunz\u00e4hligen, sehr kleinen Augen bestehen.",
            "language": {
                "name": "de",
                "url": "/api/v2/language/6/"
