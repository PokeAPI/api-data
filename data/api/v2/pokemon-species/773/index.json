        },
        {
            "flavor_text": "\u672c\u6765\u306e\u3000\u529b\u3092\u3000\u89e3\u304d\u653e\u3064\n\u6700\u5f8c\u306e\u3000\u30d5\u30a1\u30af\u30bf\u30fc\u306f\u3000\u4fe1\u983c\u3059\u308b\n\u30c8\u30ec\u30fc\u30ca\u30fc\u3068\u306e\u3000\u7d46\u3000\u3060\u3063\u305f\u3002",
