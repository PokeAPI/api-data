            }
        },
        {
            "flavor_text": "\u30e9\u30f3\u30d7\u306e\u3000\u3075\u308a\u3092\u3057\u3066\u3000\u307e\u3061\u306a\u304b\u306b\n\u3072\u305d\u3080\u3002\u3000\u3057\u304d\u304c\u3000\u3061\u304b\u3044\u3000\u3072\u3068\u3092\n\u307f\u3064\u3051\u308b\u3068\u3000\u305d\u3063\u3068\u3000\u3042\u3068\u3092\u3000\u3064\u3051\u308b\u3002",
