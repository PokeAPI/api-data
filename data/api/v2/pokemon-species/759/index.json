        {
            "flavor_text": "\u6bdb\u8338\u8338\u7684\u6bdb\u76ae\u6478\u8d77\u4f86\u89f8\u611f\u8d85\u7fa4\uff0c\n\u4f46\u7c97\u5fc3\u5927\u610f\u5730\u5411\u7260\u4f38\u51fa\u624b\u7684\u4eba\n\u57fa\u672c\u4e0a\u90fd\u6703\u906d\u5230\u7260\u7684\u5287\u70c8\u53cd\u64ca\u3002",
