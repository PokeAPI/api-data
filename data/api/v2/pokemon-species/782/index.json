            },
            "version": {
                "name": "shield",
                "url": "/api/v2/version/34/"
            }
        },
        {
            "flavor_text": "Se comunica con sus cong\u00e9neres mediante los\nsonidos que produce al golpear sus escamas,\nduras como el acero.",
            "language": {
                "name": "es",
                "url": "/api/v2/language/7/"
