        },
        {
            "flavor_text": "\uba38\ub9ac\uc758 \ube44\ub298\uc744 \ub3d9\ub8cc\ub07c\ub9ac\n\ubd80\ub52a\ud788\uba70 \uc2f8\uc6b0\ub294 \ubc95\uc744 \ubc30\uc6b4\ub2e4.\n\uc774\ub97c \ud1b5\ud574 \uae30\uc220\uacfc \uc815\uc2e0\uc744 \ub2e8\ub828\ud55c\ub2e4.",
            "language": {
                "name": "ko",
