        },
        {
            "flavor_text": "\u6575\u3092\u3000\u75fa\u308c\u3055\u305b\u3000\u6eba\u308c\u3055\u305b\u308b\u305f\u3081\n\u30d9\u30fc\u30eb\u306e\u3000\u3088\u3046\u306a\u3000\u624b\u8db3\u306b\u306f\n\u76ee\u306b\u306f\u3000\u898b\u3048\u306a\u3044\u3000\u6bd2\u91dd\u304c\u3000\u305f\u304f\u3055\u3093\u3002",
