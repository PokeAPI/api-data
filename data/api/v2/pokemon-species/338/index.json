            "version": {
                "name": "sword",
                "url": "/api/v2/version/33/"
            }
        },
        {
            "flavor_text": "\u5f53\u5b83\u65cb\u8f6c\u81ea\u5df1\u7684\u8eab\u4f53\u65f6\uff0c\n\u4f1a\u53d1\u51fa\u592a\u9633\u822c\u7684\u5149\u8292\uff0c\n\u8ba9\u654c\u4eba\u7684\u773c\u775b\u6682\u65f6\u5931\u660e\u3002",
