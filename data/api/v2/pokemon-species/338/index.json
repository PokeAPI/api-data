            },
            "version": {
                "name": "alpha-sapphire",
                "url": "/api/v2/version/26/"
            }
        },
        {
            "flavor_text": "\u304b\u3089\u3060\u3092\u3000\u304b\u3044\u3066\u3093\u3000\u3055\u305b\u308b\u3068\n\u305f\u3044\u3088\u3046\u306e\u3000\u3088\u3046\u306a\u3000\u3072\u304b\u308a\u3092\u3000\u306f\u306a\u3061\n\u3066\u304d\u306e\u3000\u3081\u3092\u3000\u304f\u3089\u307e\u305b\u308b\u3002",
