            "version": {
                "name": "shield",
                "url": "/api/v2/version/34/"
            }
        },
        {
            "flavor_text": "\u592a\u967d\u80fd\u91cf\u662f\u529b\u91cf\u7684\u4f86\u6e90\uff0c\n\u6240\u4ee5\u5728\u767d\u5929\u88e1\u5f88\u5f37\u3002\n\u65cb\u8f49\u6642\u8eab\u9ad4\u6703\u767c\u5149\u3002",
