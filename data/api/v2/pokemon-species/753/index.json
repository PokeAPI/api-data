                "url": "/api/v2/language/9/"
            },
            "version": {
                "name": "ultra-moon",
                "url": "/api/v2/version/30/"
            }
        },
        {
            "flavor_text": "\u304a\u65e5\u69d8\u306e\u3000\u5149\u304c\u3000\u5927\u597d\u304d\u3002\n\u3057\u3063\u304b\u308a\u3000\u65e5\u5149\u6d74\u3092\u3000\u3059\u308b\u3053\u3068\u3067\n\u8272\u9bae\u3084\u304b\u306b\u3000\u80b2\u3063\u3066\u3044\u304f\u306e\u3060\u3002",
