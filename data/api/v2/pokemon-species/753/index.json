        {
            "flavor_text": "\u3072\u308b\u307e\u306f\u3000\u3072\u304b\u308a\u3092\u3042\u3073\u3066\u3000\u306d\u3080\u308a\n\u3088\u308b\u306b\u3000\u306a\u308b\u3068\u3000\u3088\u308a\u3000\u3042\u3093\u305c\u3093\u306a\n\u306d\u3069\u3053\u3092\u3000\u3055\u304c\u3057\u3000\u3042\u308b\u304d\u3060\u3059\u3002",
