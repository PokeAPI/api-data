        {
            "flavor_text": "\u306b\u3063\u3061\u3085\u3046\u306e\u3000\u304b\u3064\u3069\u3046\u3067\u3000\u304b\u3089\u3060\u306e\n\u304d\u308c\u3064\u306f\u3000\u3075\u304b\u304f\u306a\u308b\u304c\u3000\u3072\u3068\u3070\u3093\u3067\n\u304d\u308c\u3064\u306e\u3000\u306a\u3044\u3000\u304b\u3089\u3060\u306b\u3000\u3082\u3069\u308b\u3002",
