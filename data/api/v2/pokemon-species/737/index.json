    "flavor_text_entries": [
        {
            "flavor_text": "\u305f\u3044\u306a\u3044\u306b\u3000\u3061\u304f\u3067\u3093\u304d\u306e\u3046\u3092\u3000\u3082\u3064\u3002\n\u30ad\u30e3\u30f3\u30d7\u306e\u3000\u3068\u304d\u306a\u3069\u3000\uff11\u3074\u304d\u3000\u3044\u308b\u3068\n\u3068\u3066\u3082\u3000\u3042\u308a\u304c\u305f\u3044\u3000\u30dd\u30b1\u30e2\u30f3\u3060\u3002",
