    "flavor_text_entries": [
        {
            "flavor_text": "\u3064\u3070\u3055\u306b\u3000\u3057\u3053\u307e\u308c\u305f\u3000\u3084\u3070\u306d\u3092\n\u3064\u304c\u3048\u3066\u306f\u306a\u3064\u3002\u3000\uff11\uff10\uff10\u30e1\u30fc\u30c8\u30eb\u3055\u304d\u306e\n\u3053\u3044\u3057\u3082\u3000\u3064\u3089\u306c\u304f\u3000\u305b\u3044\u3069\u3002",
