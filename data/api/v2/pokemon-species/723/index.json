        },
        {
            "flavor_text": "\u5c07\u88ab\u7a31\u70ba\u5203\u7fbd\u7684\u92b3\u5229\u7fbd\u6bdb\n\u5c04\u5411\u6575\u4eba\u6216\u7375\u7269\u3002\n\u57fa\u672c\u4e0a\u767e\u767c\u767e\u4e2d\u3002",
