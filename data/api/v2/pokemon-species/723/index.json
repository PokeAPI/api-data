        {
            "flavor_text": "\u6c17\u53d6\u308a\u5c4b\u3067\u3000\u6687\u304c\u3000\u3042\u308c\u3070\u3000\u7ffc\u306e\n\u624b\u5165\u308c\u3002\u3000\u7fbd\u6bdb\u306e\u3000\u6c5a\u308c\u304c\u3000\u6c17\u306b\n\u306a\u308a\u3059\u304e\u3066\u3000\u6226\u3048\u306a\u3044\u3053\u3068\u3082\u3002",
