            },
            "version": {
                "name": "ultra-sun",
                "url": "/api/v2/version/29/"
            }
        },
        {
            "flavor_text": "\u8ba9\u53cc\u624b\u53cc\u811a\u7684\u722a\u5b50\u5e26\u7535\uff0c\n\u5c06\u5bf9\u624b\u6495\u5f97\u7a00\u788e\u3002\u5373\u4f7f\u88ab\u8eb2\u5f00\uff0c\n\u4e5f\u4f1a\u7528\u98de\u6563\u7684\u7535\u51fb\u8ba9\u5176\u89e6\u7535\u3002",
