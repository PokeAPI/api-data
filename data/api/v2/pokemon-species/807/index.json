    "flavor_text_entries": [
        {
            "flavor_text": "\u308a\u3087\u3046\u3066\u3042\u3057\u306e\u3000\u30c4\u30e1\u3092\u3000\u305f\u3044\u3067\u3093\u3055\u305b\n\u3042\u3044\u3066\u3092\u3000\u3084\u3064\u3056\u304d\u3002\u3000\u304b\u308f\u3055\u308c\u3066\u3082\n\u3068\u3073\u3061\u308b\u3000\u3067\u3093\u3052\u304d\u3067\u3000\u304b\u3093\u3067\u3093\u3055\u305b\u308b\u3002",
