            }
        },
        {
            "flavor_text": "\u30bf\u30d5\u306a\u3000\u30d5\u30a1\u30a4\u30c8\u304c\u3000\u3064\u308a\u3073\u3068\u306b\n\u3060\u3044\u306b\u3093\u304d\u3002\u3000\u3080\u3060\u3093\u3067\u3000\u3044\u3051\u306b\n\u306f\u306a\u3059\u306e\u3067\u3000\u3069\u3093\u3069\u3093\u3000\u3075\u3048\u308b\u3002",
