        },
        {
            "flavor_text": "\u4ece\u52fe\u5e15\u8def\u7fc1\u3001\u4ee3\u62c9\u57fa\u7fc1\u548c\n\u6bd5\u529b\u5409\u7fc1\u90a3\u91cc\u5b66\u4f1a\u4e86\u6218\u6597\u3002\n\u9a70\u9a8b\u5168\u4e16\u754c\uff0c\u4e0d\u65ad\u4fee\u884c\u3002",
