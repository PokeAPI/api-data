    "flavor_text_entries": [
        {
            "flavor_text": "\u6027\u683c\u51f6\u66b4\u7684\u5b9d\u53ef\u68a6\u3002\n\u4f1a\u7528\u80fd\u54ac\u65ad\u94c1\u68cd\u7684\u989a\u90e8\n\u5927\u53e3\u54ac\u4f4f\u730e\u7269\u3002",
