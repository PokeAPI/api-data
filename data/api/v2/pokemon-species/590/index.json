            }
        },
        {
            "flavor_text": "\u308a\u3087\u3046\u3066\u304b\u3089\u3000\u3075\u304d\u3060\u3059\u3000\u307b\u3046\u3057\u306f\n\u3082\u3046\u3069\u304f\u3060\u304c\u3000\u3088\u304f\u3000\u304b\u308f\u304b\u305b\u3070\n\u304a\u306a\u304b\u306e\u3000\u304f\u3059\u308a\u306b\u3000\u306a\u308b\u3068\u3000\u3044\u3046\u3002",
