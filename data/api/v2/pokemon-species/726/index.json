        {
            "flavor_text": "\uc55e\ubc1c\uc758 \ud798\uc774 \uc790\ub791\uac70\ub9ac\ub2e4.\n\ud380\uce58 \ud55c \ubc29\uc5d0 \ucca0\ubd09\ub3c4\n\uad6c\ubd80\ub7ec\ud2b8\ub824 \ubc84\ub9b0\ub2e4.",
