    "flavor_text_entries": [
        {
            "flavor_text": "\u4ee5\u524d\u80a2\u7684\u529b\u91cf\u800c\u81ea\u8c6a\u3002\n\u4e00\u8bb0\u62f3\u51fb\uff0c\u8fde\u94c1\u68d2\u90fd\u80fd\u5f2f\u6298\u3002",
