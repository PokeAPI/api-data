            "version": {
                "name": "shield",
                "url": "/api/v2/version/34/"
            }
        },
        {
            "flavor_text": "Enfrentarse a un adversario formidable aviva\ntanto su esp\u00edritu combativo como la energ\u00eda\nt\u00e9rmica de su cascabel.",
            "language": {
                "name": "es",
                "url": "/api/v2/language/7/"
