        {
            "flavor_text": "\u304a\u306a\u304b\u306e\u3000\u305f\u3044\u3082\u3046\u306f\u3000\u307b\u304a\u3093\u305b\u3044\u306b\n\u3059\u3050\u308c\u308b\u3002\u3000\u3080\u304b\u3057\u306e\u3000\u3072\u3068\u306f\u3000\u306c\u3051\u305f\n\u305f\u3044\u3082\u3046\u3067\u3000\u307c\u3046\u304b\u3093\u304e\u3092\u3000\u3064\u304f\u3063\u305f\u3002",
