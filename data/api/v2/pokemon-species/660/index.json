        },
        {
            "flavor_text": "\u304a\u8179\u306e\u3000\u4f53\u6bdb\u306f\u3000\u4fdd\u6e29\u6027\u306b\n\u512a\u308c\u308b\u3002\u3000\u6614\u306e\u3000\u4eba\u306f\u3000\u629c\u3051\u305f\n\u4f53\u6bdb\u3067\u3000\u9632\u5bd2\u7740\u3092\u3000\u3064\u304f\u3063\u305f\u3002",
