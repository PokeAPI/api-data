    "flavor_text_entries": [
        {
            "flavor_text": "\u3044\u3084\u3057\u3068\u3000\u3081\u3050\u307f\u306e\u3000\u3061\u304b\u3089\u3092\u3000\u3082\u3064\n\u3058\u3042\u3044\u306b\u3000\u307f\u3061\u305f\u3000\u30dd\u30b1\u30e2\u30f3\u3002\u3000\u306f\u308b\u304b\n\u3080\u304b\u3057\u3000\u30ac\u30e9\u30eb\u306b\u3000\u304f\u3093\u308a\u3093\u3057\u3066\u3044\u305f\u3002",
            "language": {
