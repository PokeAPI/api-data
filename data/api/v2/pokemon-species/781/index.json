        {
            "flavor_text": "\u6d77\u306b\u3000\u6f02\u3046\u3000\u30e2\u30ba\u30af\u306e\u3000\u9b42\u304c\n\u751f\u307e\u308c\u5909\u308f\u3063\u305f\u3002\u3000\u6d77\u306e\u3000\u30b4\u30df\u3092\n\u30e2\u30ba\u30af\u306b\u3000\u7d61\u3081\u3066\u3000\u8eab\u4f53\u3092\u3000\u4fdd\u3064\u3002",
