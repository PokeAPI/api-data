        },
        {
            "flavor_text": "\u3046\u307f\u306b\u3000\u3044\u304b\u308a\u3092\u3000\u304a\u308d\u3057\u3066\u3000\u3048\u3082\u306e\u3092\n\u307e\u3061\u3076\u305b\u3002\u3000\u304a\u304a\u3082\u306e\u306e\u3000\u30db\u30a8\u30eb\u30aa\u30fc\u3092\n\u3064\u304b\u307e\u3048\u3066\u3000\u305b\u3044\u304d\u3092\u3000\u3059\u3044\u3068\u308b\u3002",
