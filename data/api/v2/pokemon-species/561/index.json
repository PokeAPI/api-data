        },
        {
            "flavor_text": "\u30b7\u30f3\u30dc\u30e9\u30fc\u304c\u3000\u98db\u3076\u3000\u7802\u6f20\u306e\u4e0b\u3092\n\u8abf\u67fb\u3059\u308b\u3068\u3000\u53e4\u4ee3\u306e\u3000\u90fd\u5e02\u3068\n\u601d\u308f\u308c\u308b\u3000\u540d\u6b8b\u308a\u304c\u3000\u898b\u3064\u304b\u3063\u305f\u3002",
