            },
            "version": {
                "name": "sword",
                "url": "/api/v2/version/33/"
            }
        },
        {
            "flavor_text": "\u30b7\u30f3\u30dc\u30e9\u30fc\u304c\u3000\u3068\u3076\u3000\u3055\u3070\u304f\u306e\u3057\u305f\u3092\n\u3061\u3087\u3046\u3055\u3059\u308b\u3068\u3000\u3053\u3060\u3044\u306e\u3000\u3068\u3057\u3068\n\u304a\u3082\u308f\u308c\u308b\u3000\u306a\u3054\u308a\u304c\u3000\u307f\u3064\u304b\u3063\u305f\u3002",
