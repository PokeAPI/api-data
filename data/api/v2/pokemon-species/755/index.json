    "flavor_text_entries": [
        {
            "flavor_text": "\u3066\u3093\u3081\u3064\u3057\u306a\u304c\u3089\u3000\u3072\u304b\u308b\u3000\u307b\u3046\u3057\u3092\n\u3042\u305f\u308a\u306b\u3000\u3070\u3089\u307e\u304f\u3002\u3000\u305d\u306e\u3072\u304b\u308a\u3092\n\u307f\u305f\u3082\u306e\u306f\u3000\u3075\u304b\u3044\u306d\u3080\u308a\u306b\u3000\u304a\u3061\u308b\u3002",
