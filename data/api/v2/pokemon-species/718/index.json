                "url": "/api/v2/language/11/"
            },
            "version": {
                "name": "ultra-moon",
                "url": "/api/v2/version/30/"
            }
        },
        {
            "flavor_text": "\u636e\u8bf4\uff0c\u5728\u4e0e\u5a01\u80c1\u5230\u751f\u6001\u7cfb\u7edf\u8005\u6218\u6597\u65f6\uff0c\n\u5b83\u4f1a\u53d8\u5316\u4e3a\u66f4\u5f3a\u5927\u7684\u6837\u5b50\u3002",
