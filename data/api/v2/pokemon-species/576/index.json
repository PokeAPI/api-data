        },
        {
            "flavor_text": "\u30b4\u30c1\u30eb\u30bc\u30eb\u306b\u3000\u3058\u3076\u3093\u306e\u3000\u3055\u3044\u3054\u3092\n\u307f\u305b\u3089\u308c\u305f\u3000\u3042\u304f\u306b\u3093\u306f\u3000\u305d\u306e\u3072\u3092\n\u3055\u3044\u3054\u306b\u3000\u3086\u304f\u3048\u3092\u3000\u305f\u3063\u305f\u3002",
