    "flavor_text_entries": [
        {
            "flavor_text": "\u304a\u304f\u3073\u3087\u3046\u3067\u3000\u305f\u304f\u3055\u3093\u306e\u3000\u3042\u3057\u3092\n\u3070\u305f\u3064\u304b\u305b\u3000\u3072\u3063\u3057\u306b\u3000\u306b\u3052\u307e\u308f\u308b\u3002\n\u306b\u3052\u305f\u3042\u3068\u306f\u3000\u30d4\u30ab\u30d4\u30ab\u3000\u304d\u308c\u3044\u3002",
