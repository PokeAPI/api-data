                "url": "/api/v2/language/12/"
            },
            "version": {
                "name": "ultra-moon",
                "url": "/api/v2/version/30/"
            }
        },
        {
            "flavor_text": "\u3057\u3093\u306e\u3000\u304d\u3087\u3046\u3066\u304d\u3068\u3000\u3042\u3046\u3068\n\u307f\u304c\u308b\u306b\u3000\u306a\u308b\u305f\u3081\u3000\u308a\u3087\u3046\u3066\u306e\n\u3051\u3092\u3000\u304b\u307f\u3061\u304e\u308a\u3000\u3059\u3066\u3066\u3057\u307e\u3046\u3002",
            "language": {
                "name": "ja-Hrkt",
                "url": "/api/v2/language/1/"
            },
            "version": {
                "name": "sword",
                "url": "/api/v2/version/33/"
            }
        },
        {
            "flavor_text": "\uc9c4\uc815\ud55c \uac15\uc801\uacfc \ub9cc\ub098\uba74\n\ubab8\uc744 \uac00\ubccd\uac8c \ud558\uae30 \uc704\ud574\n\uc591\uc190\uc758 \ud138\uc744 \ubb3c\uc5b4\ub72f\uc5b4 \ubc84\ub9b0\ub2e4.",
