                "url": "/api/v2/language/11/"
            },
            "version": {
                "name": "shield",
                "url": "/api/v2/version/34/"
            }
        },
        {
            "flavor_text": "\u636e\u8bf4\u90a3\u4e9b\u80fd\u591f\u6539\u53d8\u5386\u53f2\n\u7684\u91cd\u5927\u4e8b\u4ef6\u5176\u5b9e\u90fd\u548c\n\u4e4c\u8d3c\u738b\u7684\u50ac\u7720\u80fd\u529b\u6709\u5173\u3002",
