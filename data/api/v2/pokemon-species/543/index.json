            "flavor_text": "\u30e4\u30af\u30c7\u306f\u3000\u7a2e\u985e\u306e\u3000\u8fd1\u3044\n\u4ef2\u9593\u3060\u304c\u3000\u304a\u305f\u304c\u3044\u304c\u3000\u51fa\u4f1a\u3046\u3068\n\u5927\u3052\u3093\u304b\u306b\u3000\u306a\u308b\u305e\u3002",
            "language": {
                "name": "ja",
                "url": "/api/v2/language/11/"
            },
            "version": {
                "name": "sword",
                "url": "/api/v2/version/33/"
            }
        },
        {
            "flavor_text": "\u867d\u7136\u4e0e\u70e7\u706b\u86a3\u662f\u79cd\u7c7b\n\u76f8\u8fd1\u7684\u4f19\u4f34\uff0c\u4f46\u662f\n\u4e00\u89c1\u9762\u5c31\u4f1a\u5927\u6253\u4e00\u756a\u3002",
