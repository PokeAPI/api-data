        },
        {
            "flavor_text": "\u3088\u304f\u3000\u306a\u3064\u304f\u304c\u3000\u304b\u307f\u3050\u305b\u304c\n\u3042\u308b\u306e\u3067\u3000\u305a\u3063\u3068\u3000\u305d\u3060\u3066\u308b\u306b\u306f\n\u306a\u304b\u306a\u304b\u3000\u307b\u306d\u304c\u3000\u304a\u308c\u308b\u305e\u3002",
