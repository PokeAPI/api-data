        },
        {
            "flavor_text": "\u7576\u7260\u7ad9\u5728\u96fb\u8996\u6a5f\u65c1\u908a\u6642\uff0c\n\u756b\u9762\u4e0a\u6703\u51fa\u73fe\u5947\u602a\u7684\u666f\u8272\u3002\n\u64da\u8aaa\u90a3\u662f\u7260\u7684\u6545\u9109\u3002",
