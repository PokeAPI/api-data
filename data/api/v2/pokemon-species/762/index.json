            }
        },
        {
            "flavor_text": "\u8e66\u8e66\u8df3\u8df3\u5730\u56db\u5904\u884c\u52a8\uff0c\n\u6325\u821e\u7740\u5934\u90e8\u7684\u58f3\u76d6\u3002\n\u5982\u679c\u88ab\u649e\u5230\u4e86\u4f1a\u975e\u5e38\u75bc\u3002",
