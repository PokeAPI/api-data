    "flavor_text_entries": [
        {
            "flavor_text": "\u307f\u3092\u3000\u307e\u3082\u308b\u305f\u3081\u3000\u30d8\u30bf\u304c\u3000\u306f\u3063\u305f\u3064\u3002\n\u304b\u306a\u308a\u306e\u3000\u304b\u305f\u3055\u3067\u3000\u3068\u308a\u30dd\u30b1\u30e2\u30f3\u306b\n\u3064\u3064\u304b\u308c\u3066\u3082\u3000\u305c\u3093\u305c\u3093\u3000\u3078\u3044\u304d\u3060\u3002",
