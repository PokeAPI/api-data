    "flavor_text_entries": [
        {
            "flavor_text": "\u30c8\u30c3\u30d7\u3092\u3000\u3081\u3056\u3059\u306f\u305a\u304c\u3000\u3086\u304d\u3084\u307e\u306b\n\u307e\u3088\u3044\u306e\u307c\u308a\u3000\u3055\u3080\u3055\u306b\u3000\u305f\u3048\u308b\u3046\u3061\u306b\n\u3051\u304c\u3000\u306f\u3048\u3066\u304d\u3066\u3000\u3057\u3093\u304b\u3057\u3066\u3044\u305f\u3002",
