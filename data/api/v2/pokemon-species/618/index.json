        },
        {
            "flavor_text": "\u304b\u3089\u3060\u306e\u3000\u304d\u3044\u308d\u3044\u3000\u3082\u3088\u3046\u304b\u3089\n\u3064\u3088\u3044\u3000\u3067\u3093\u304d\u3092\u3000\u306f\u306a\u3064\u3068\u304d\n\u306a\u305c\u304b\u3000\u3059\u3053\u3057\u3000\u307b\u307b\u3048\u3093\u3067\u3044\u308b\u3002",
