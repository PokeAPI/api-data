    "flavor_text_entries": [
        {
            "flavor_text": "\u4f7f\u7528\u575a\u786c\u7684\u6811\u679c\u6218\u6597\u3002\n\u6bcf\u4e00\u4ee3\u7684\u8001\u5927\u4f1a\u628a\u8fd9\u4e2a\u6280\u672f\n\u4f20\u627f\u7ed9\u56e2\u961f\u7684\u4f19\u4f34\u4eec\u3002",
