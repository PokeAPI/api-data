        },
        {
            "flavor_text": "\uff12\uff10\u5339\u524d\u5f8c\u306e\u3000\u30b0\u30eb\u30fc\u30d7\u3092\u3000\u3064\u304f\u308b\u3002\n\u305d\u306e\u7d50\u675f\u306f\u3000\u975e\u5e38\u306b\u3000\u56fa\u304f\n\u7d76\u5bfe\u3000\u4ef2\u9593\u3092\u3000\u898b\u6368\u3066\u306a\u3044\u3002",
