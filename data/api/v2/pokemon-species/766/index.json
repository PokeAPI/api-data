            },
            "version": {
                "name": "ultra-moon",
                "url": "/api/v2/version/30/"
            }
        },
        {
            "flavor_text": "\u30dc\u30b9\u306e\u3000\u3057\u3058\u306b\u3000\u3057\u305f\u304c\u3044\u3000\u307f\u3054\u3068\u306a\n\u30c1\u30fc\u30e0\u30d7\u30ec\u30a4\u3067\u3000\u3053\u3046\u3076\u3064\u306e\u3000\u304d\u306e\u307f\u3092\n\u3055\u304c\u3057\u3066\u3000\u306a\u304b\u307e\u3069\u3046\u3057\u3000\u305f\u3059\u3051\u3042\u3046\u3002",
