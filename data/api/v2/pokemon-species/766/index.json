        },
        {
            "flavor_text": "\u304d\u306e\u307f\u306f\u3000\u3076\u304d\u3067\u3000\u3042\u308a\u3000\u3057\u3085\u3057\u3087\u304f\u3002\n\u3080\u308c\u306e\u3000\u306a\u304b\u3067\u3000\uff11\u3070\u3093\u3000\u3068\u304a\u304f\u307e\u3067\n\u304d\u306e\u307f\u3092\u3000\u306a\u3052\u3089\u308c\u308b\u3082\u306e\u304c\u3000\u30dc\u30b9\u3060\u3002",
