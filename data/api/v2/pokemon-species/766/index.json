                "name": "en",
                "url": "/api/v2/language/9/"
            },
            "version": {
                "name": "ultra-sun",
                "url": "/api/v2/version/29/"
            }
        },
        {
            "flavor_text": "\u553e\u6db2\u3092\u3000\u4f7f\u3063\u3066\u3000\u8449\u3063\u3071\u3092\u3000\u80a9\u306b\n\u8cbc\u308a\u4ed8\u3051\u3066\u3000\u30de\u30fc\u30ad\u30f3\u30b0\u3002\u3000\u8449\u3063\u3071\u306e\n\u914d\u7f6e\u3067\u3000\u3069\u306e\u3000\u7fa4\u308c\u304b\u3000\u308f\u304b\u308b\u3088\u3002",
