            "version": {
                "name": "sword",
                "url": "/api/v2/version/33/"
            }
        },
        {
            "flavor_text": "\u4ece\u5316\u77f3\u590d\u539f\u800c\u6765\u3002\n\u636e\u8bf4\u5f53\u51b0\u96ea\u5de8\u9f99\u543c\u53eb\u65f6\uff0c\n\u591c\u7a7a\u4e2d\u5c31\u4f1a\u51fa\u73b0\u6781\u5149\u3002",
            "language": {
                "name": "zh-Hans",
                "url": "/api/v2/language/12/"
            },
            "version": {
                "name": "sword",
                "url": "/api/v2/version/33/"
            }
        },
        {
            "flavor_text": "\u306f\u3052\u3057\u3044\u3000\u3044\u304b\u308a\u306b\u3000\u304b\u3089\u308c\u308b\u3068\n\u3082\u3046\u308c\u3064\u306a\u3000\u308c\u3044\u304d\u3092\u3000\u3075\u304d\u3060\u3057\u3066\n\u3042\u305f\u308a\u3044\u3061\u3081\u3093\u3092\u3000\u3053\u304a\u308a\u3065\u3051\u306b\u3059\u308b\u3002",
