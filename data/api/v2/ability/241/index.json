            "version_group": {
                "name": "sword-shield",
                "url": "/api/v2/version-group/20/"
            }
        },
        {
            "flavor_text": "Quando usa Surf o Sub, il Pok\u00e9mon cattura una preda.\nSe subisce dei danni, la sputa fuori per attaccare.",
            "language": {
                "name": "it",
                "url": "/api/v2/language/8/"
