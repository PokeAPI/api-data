            "version_group": {
                "name": "sword-shield",
                "url": "/api/v2/version-group/20/"
            }
        },
        {
            "flavor_text": "Tras usar Surf o Buceo, emerge con una presa en la\nboca. Al recibir da\u00f1o, ataca escupi\u00e9ndola al rival.",
            "language": {
                "name": "es",
                "url": "/api/v2/language/7/"
