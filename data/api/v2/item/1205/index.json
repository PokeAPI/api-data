            }
        },
        {
            "language": {
                "name": "ko",
                "url": "/api/v2/language/3/"
            },
            "text": "\uba38\ub9ac\ub97c \ube44\uc6cc\uc11c \uc21c\uac04\uc801\uc73c\ub85c\n\ubb34\uc5b8\uac00\ub97c \uc78a\uc5b4\ubc84\ub9bc\uc73c\ub85c\uc368\n\uc790\uc2e0\uc758 \ud2b9\uc218\ubc29\uc5b4\ub97c \ud06c\uac8c \uc62c\ub9b0\ub2e4.",
            "version_group": {
