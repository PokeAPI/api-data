        {
            "language": {
                "name": "ko",
                "url": "/api/v2/language/3/"
            },
            "text": "\ud2b9\uc218\ud55c \ub3c5\uc561\uc744 \ub07c\uc5b9\ub294\ub2e4.\n\ub3c5 \uc0c1\ud0dc\uc778 \uc0c1\ub300\ub294\n\uacf5\uaca9, \ud2b9\uc218\uacf5\uaca9, \uc2a4\ud53c\ub4dc\uac00 \ub5a8\uc5b4\uc9c4\ub2e4.",
            "version_group": {
