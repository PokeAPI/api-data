            }
        },
        {
            "language": {
                "name": "it",
                "url": "/api/v2/language/8/"
            },
            "text": "Colpisce il bersaglio con una debole scarica\nelettrica che lo paralizza.",
            "version_group": {
                "name": "sword-shield",
                "url": "/api/v2/version-group/20/"
            }
        },
        {
            "language": {
                "name": "en",
