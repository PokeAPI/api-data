            }
        },
        {
            "language": {
                "name": "ko",
                "url": "/api/v2/language/3/"
            },
            "text": "\uc57d\ud55c \uc804\uaca9\uc744 \ub0a0\ub824\uc11c\n\uc0c1\ub300\ub97c \ub9c8\ube44 \uc0c1\ud0dc\ub85c \ub9cc\ub4e0\ub2e4.",
            "version_group": {
                "name": "sword-shield",
