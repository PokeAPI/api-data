            }
        },
        {
            "language": {
                "name": "fr",
                "url": "/api/v2/language/5/"
            },
            "text": "Envoie une rafale de dards.\nPeut toucher de deux \u00e0 cinq fois.",
            "version_group": {
                "name": "sword-shield",
                "url": "/api/v2/version-group/20/"
            }
