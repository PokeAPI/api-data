        },
        {
            "language": {
                "name": "it",
                "url": "/api/v2/language/8/"
            },
            "text": "Fiamme sinistre e misteriose causano\nuna scottatura al bersaglio.",
            "version_group": {
                "name": "sword-shield",
                "url": "/api/v2/version-group/20/"
            }
        },
        {
            "language": {
