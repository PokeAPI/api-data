    "flavor_text_entries": [
        {
            "language": {
                "name": "en",
                "url": "/api/v2/language/9/"
            },
            "text": "A thirst-quenching\ntea prepared by an\nold lady.",
            "version_group": {
                "name": "emerald",
                "url": "/api/v2/version-group/6/"
            }
        },
        {
            "language": {
                "name": "en",
                "url": "/api/v2/language/9/"
            },
            "text": "An aromatic tea prepared by an old\nlady. It will slake even the worst\nthirst.",
            "version_group": {
                "name": "firered-leafgreen",
                "url": "/api/v2/version-group/7/"
            }
        },
        {
            "language": {
                "name": "ja-Hrkt",
                "url": "/api/v2/language/1/"
            },
            "text": "\u3059\u3053\u3057\u3000\u306b\u304c\u304f\u3066\n\u3053\u3046\u3070\u3057\u3044\u3000\u304b\u304a\u308a\u306e\u3000\u304a\u3061\u3083\u3002\n\u304b\u308f\u3044\u305f\u3000\u306e\u3069\u304c\u3000\u3046\u308b\u304a\u3046\u3002",
            "version_group": {
                "name": "lets-go-pikachu-lets-go-eevee",
                "url": "/api/v2/version-group/19/"
            }
        },
        {
            "language": {
                "name": "ko",
                "url": "/api/v2/language/3/"
            },
            "text": "\uc57d\uac04 \uc501\uc4f8\ud558\uace0\n\uad6c\uc218\ud55c \ud5a5\uae30\uac00 \ub098\ub294 \ucc28.\n\ub9d0\ub790\ub358 \ubaa9\uc774 \ucd09\ucd09\ud574\uc9c4\ub2e4.",
