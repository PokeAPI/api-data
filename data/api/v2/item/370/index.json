            }
        },
        {
            "language": {
                "name": "ko",
                "url": "/api/v2/language/3/"
            },
            "text": "\ud798\uc744 \ubaa8\uc544 \uacf5\uaca9\ud55c\ub2e4.\n\uc0c1\ub300\ubcf4\ub2e4 \ub098\uc911\uc5d0 \uc4f0\uba74\n\uae30\uc220\uc758 \uc704\ub825\uc740 2\ubc30\uac00 \ub41c\ub2e4.",
            "version_group": {
