            }
        },
        {
            "language": {
                "name": "ko",
                "url": "/api/v2/language/3/"
            },
            "text": "\uc790\uc2e0\uc774 \ubc30\uc6b4 \uae30\uc220 \uc911\n\ud558\ub098\ub97c \ubb34\uc791\uc704\ub85c \ub0a0\ub9b0\ub2e4.\n\uc790\uc2e0\uc774 \uc7a0\ub4e4\uc5b4 \uc788\uc744 \ub54c\ub9cc \uc4f8 \uc218 \uc788\ub2e4.",
            "version_group": {
