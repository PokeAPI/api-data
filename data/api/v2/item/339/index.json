        },
        {
            "language": {
                "name": "ko",
                "url": "/api/v2/language/3/"
            },
            "text": "5\ud134 \ub3d9\uc548 \uc2f8\ub77c\uae30\ub208\uc744 \ub0b4\ub9ac\uac8c \ud574\uc11c\n\uc5bc\uc74c\ud0c0\uc785\uc774 \uc544\ub2cc \ud3ec\ucf13\ubaac\n\ubaa8\ub450\uc5d0\uac8c \ub370\ubbf8\uc9c0\ub97c \uc900\ub2e4.",
            "version_group": {
                "name": "sword-shield",
