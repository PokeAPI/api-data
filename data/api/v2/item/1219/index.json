            }
        },
        {
            "language": {
                "name": "zh-Hant",
                "url": "/api/v2/language/4/"
            },
            "text": "\u5c07\u5805\u786c\u7684\u5c3e\u5df4\n\u7529\u5411\u5c0d\u624b\u9032\u884c\u653b\u64ca\u3002\n\u6709\u6642\u6703\u964d\u4f4e\u5c0d\u624b\u7684\u9632\u79a6\u3002",
            "version_group": {
                "name": "sword-shield",
                "url": "/api/v2/version-group/20/"
            }
        },
        {
            "language": {
                "name": "fr",
                "url": "/api/v2/language/5/"
            },
            "text": "Attaque l\u2019ennemi avec une queue de fer.\nPeut aussi baisser sa D\u00e9fense.",
            "version_group": {
                "name": "sword-shield",
                "url": "/api/v2/version-group/20/"
            }
        },
        {
            "language": {
                "name": "de",
