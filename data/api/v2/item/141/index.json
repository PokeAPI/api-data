        },
        {
            "language": {
                "name": "ja-Hrkt",
                "url": "/api/v2/language/1/"
            },
            "text": "\u304b\u3063\u3053\u3088\u3055\u306b\u3000\u307f\u304c\u304d\u3092\u3000\u304b\u3051\u308b\u3000\u305f\u3081\u306e\n\u30dd\u30ed\u30c3\u30af\u3092\u3000\u3064\u304f\u308b\u3068\u304d\u3000\u3064\u304b\u308f\u308c\u308b\u3002\n\u3042\u304b\u3044\u308d\u306e\u3000\u307f\u306f\u3000\u305f\u3079\u308b\u3068\u3000\u304b\u3089\u3044\u3002",
