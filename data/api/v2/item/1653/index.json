        },
        {
            "language": {
                "name": "ko",
                "url": "/api/v2/language/3/"
            },
            "text": "\ud48d\uc694\uc758 \uc655\uc758 \uc560\ub9c8\uac00\n\uc990\uaca8 \uba39\ub294 \uc791\ubb3c.\n\ub208 \ub36e\uc778 \ubc2d\uc5d0\uc11c \uc790\ub790\ub2e4.",
            "version_group": {
